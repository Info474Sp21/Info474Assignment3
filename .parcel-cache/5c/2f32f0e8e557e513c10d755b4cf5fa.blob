*{
    margin: 0;
    padding: 0;
}

html {
    background: #C4CED4;
    color: #0C2C56;
    line-height: 200%;
}
.container{
    margin: 5%;
}
.flexed {
    display: flex;
}
.left-side, .right-side{
    width: 10%;
}
.data-center{
    width: 80%;
}
.centered{
    margin: auto;
    padding: 10px;
    text-align: center;
}
button{
    width: 100%;
    outline: none;
    text-emphasis: none;
    text-decoration: none;
    height: 100%;
    border: none;
    background: #005C5C;
    color: #D50032;
    opacity: 0.7;
    animation-duration: 1100ms;
}
button:hover{
    opacity: 1.0;
}
.vis-description{
    text-decoration: #0C2C56;
}

